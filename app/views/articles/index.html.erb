<h1>All Articles</h1>

<ul id="articles">
  <% @articles.each do |article| %>
    <li>
      <%= link_to article.title, article_path(article), class: "article_title" %>
    </li>
  <% end %>
</ul>
<br>
<p>

<% if logged_in? %>
  <%= link_to "Create a new article", new_article_path, class: "article_new" %>
<% end %>
<%= link_to "View site tags", tags_path %>